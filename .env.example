# PostgreSQL
POSTGRES_DB=memehub
POSTGRES_USER=memehub
POSTGRES_PASSWORD=zmien_na_silne_haslo

# Express Session (wygeneruj: openssl rand -hex 32)
SESSION_SECRET=zmien_na_64_znakowy_losowy_string

# URL bazy danych (lokalny development)
DATABASE_URL=postgresql://memehub:zmien_na_silne_haslo@localhost:5432/memehub

# URL frontendu (dla CORS) - wpisz adres VPS lub domeny
FRONTEND_URL=http://localhost

# Port aplikacji
APP_PORT=80

# Gemini API (dla MemeStudio i RoastStation)
GEMINI_API_KEY=twoj_klucz_gemini_api

# Vite (zostawić puste - nginx proxy obsługuje /api)
VITE_API_URL=
